{
  "version": 2,
  "builds": [
    {
      "src": "**/*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/img/(.*)",
      "dest": "/img/$1"
    },
    {
      "src": "/error/404/null.html",
      "dest": "/error/404/null.html"
    },
    {
      "src": "/(.*)",
      "dest": "/error/404/null.html",
      "status": 404
    }
  ]
}
