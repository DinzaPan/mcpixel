{
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/error/404\\.html",
      "dest": "/error/404.html",
      "status": 200
    },
    {
      "src": "/(.*)",
      "dest": "/error/404.html",
      "status": 404
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/sc/:path",
      "destination": "/sc/:path.html"
    },
    {
      "source": "/:path",
      "destination": "/:path.html"
    }
  ],
  "redirects": [
    {
      "source": "/index.html",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/sc/:path.html",
      "destination": "/sc/:path",
      "permanent": true
    },
    {
      "source": "/:path.html",
      "destination": "/:path",
      "permanent": true
    }
  ]
}
