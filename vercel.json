{
  "cleanUrls": true,
  "trailingSlash": false,
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/error/404\\.html",
      "dest": "/error/404.html",
      "status": 200
    },
    {
      "src": "/(.*)",
      "dest": "/error/404.html",
      "status": 404
    }
  ],
  "rewrites": [
    {
      "source": "/sc/:path*",
      "destination": "/sc/:path*.html"
    },
    {
      "source": "/:page",
      "destination": "/:page.html"
    }
  ]
}
