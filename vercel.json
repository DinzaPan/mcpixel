{
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/sc/:path",
      "destination": "/sc/:path.html"
    },
    {
      "source": "/:path",
      "destination": "/:path.html"
    }
  ],
  "redirects": [
    {
      "source": "/index.html",
      "destination": "/",
      "permanent": true
    }
  ]
}
